    "@types/refractor": "^3.0.2",
    "concurrently": "^7.6.0",
    "eslint": "^8.53.0",
    "eslint-config-next": "^14.0.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^15.16.2",
    "@react-hook/mouse-position": "^4.1.3",
    "@vercel/analytics": "^1.1.1",
